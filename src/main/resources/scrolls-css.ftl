div {
    /*border: 1px solid black;*/
}
h1 {
    color: #4c528c;
    margin-bottom: 0;
    padding-bottom: 0;
}
h2 {
    background-color: #778899;
    color: #ffffff;
    padding: 5px;
    margin: 10px 0 0;
    border-bottom: 2px solid #a9a9a9;
    font-size: 1.2em;
}
th {
    background-color: #d3d3d3;
    padding-left: 5px;
    padding-right: 5px;
}
td {
    vertical-align: top;
}
ul {
    margin-top: 5px;
    list-style: none;
}
.section {
    border: 1px solid #a9a9a9;
}
.label{
    font-weight: bold;
}
.left {
    float: left;
    width: 50%;
}
.right {
    float: right;
    width: 49%;
}
.clear {
    clear: both;
}
.change {
    background-color: #778899;
    padding-left: 5px;
    padding-right: 5px;
    border-radius: 3px;
    color: white;

}
.closed {
    text-decoration: line-through;
}
.open {

}
.resolved {
    text-decoration: line-through;
}
.warning {
    color: red;
}
#changes td,th {
    text-align: left;
}

.author {
    display: block;
    white-space: nowrap;
}

#commitLog {
    margin-top: 10px;
}
.commit {
    margin-top: 10px;
}
.commitHeader {
    background-color: #d3d3d3;
    padding-left: 5px;
    padding-right: 5px;
}
.commitMessage {
    padding-left: 5px;
    padding-right: 5px;
}
.commitAuthor {
    margin-left: 10px;
}
.commitDate {
    margin-left: 10px;
}
.commitDetails {
    margin-top: 10px;
}
.show {
    display: block;
}
.hide {
    display: none;
}
.jiraTitle {
    margin-left: 15px;
}
#quality td {
    padding-left: 7px;
}
.small {
    font-size: 0.8em;
}
